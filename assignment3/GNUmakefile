$(REMAINING_TARGETS):
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.Torrent_Idl $(@)
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.Torrent_Peer $(@)
	$(KEEP_GOING)@$(MAKE) -f GNUmakefile.Torrent_Tracker $(@)

.PHONY: Torrent_Idl
